{
  "connection": "default",
  "collectionName": "plats",
  "info": {
    "name": "plats"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "nom": {
      "type": "string"
    },
    "menus": {
      "collection": "menu",
      "via": "plats"
    },
    "Description": {
      "type": "text"
    },
    "Prix": {
      "type": "biginteger"
    },
    "fournisseur": {
      "type": "string"
    },
    "jour": {
      "type": "string"
    },
    "restaurants": {
      "via": "plats",
      "collection": "restaurants"
    }
  }
}
